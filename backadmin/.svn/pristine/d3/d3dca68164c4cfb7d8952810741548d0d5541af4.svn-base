public class ResponseUser implements Serializable {

	private static final long serialVersionUID = 7823845511811711159L;
	private String userId;
	private String mobileNum;
	private Double totalInvestMoney;
	private Double followInvestTotalMoney;
	private String realName;
	private String investTime;
	private Double awardMoney;

	/**
	 * 
	 */
	public ResponseUser() {
		super();
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param userId
	 * @param mobileNum
	 * @param totalInvestMoney
	 * @param realName
	 */
	public ResponseUser(String userId, String mobileNum,
			Double totalInvestMoney, String realName,
			Double followInvestTotalMoney) {
		super();
		this.userId = userId;
		this.mobileNum = mobileNum;
		this.totalInvestMoney = totalInvestMoney;
		this.realName = realName;
		this.followInvestTotalMoney = followInvestTotalMoney;
	}

	/**
	 * @return the userId
	 */
	public String getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(String userId) {
		this.userId = userId;
	}

	/**
	 * @return the mobileNum
	 */
	public String getMobileNum() {
		return mobileNum;
	}

	/**
	 * @param mobileNum
	 *            the mobileNum to set
	 */
	public void setMobileNum(String mobileNum) {
		this.mobileNum = mobileNum;
	}

	/**
	 * @return the totalInvestMoney
	 */
	public Double getTotalInvestMoney() {
		return totalInvestMoney;
	}

	/**
	 * @param totalInvestMoney
	 *            the totalInvestMoney to set
	 */
	public void setTotalInvestMoney(Double totalInvestMoney) {
		this.totalInvestMoney = totalInvestMoney;
	}

	/**
	 * @return the realName
	 */
	public String getRealName() {
		return realName;
	}

	/**
	 * @param realName
	 *            the realName to set
	 */
	public void setRealName(String realName) {
		this.realName = realName;
	}

	/**
	 * @return the investTime
	 */
	public String getInvestTime() {
		return investTime;
	}

	/**
	 * @param investTime
	 *            the investTime to set
	 */
	public void setInvestTime(String investTime) {
		this.investTime = investTime;
	}

	/**
	 * @return the awardMoney
	 */
	public Double getAwardMoney() {
		return awardMoney;
	}

	/**
	 * @param awardMoney
	 *            the awardMoney to set
	 */
	public void setAwardMoney(Double awardMoney) {
		this.awardMoney = awardMoney;
	}

	/**
	 * @return the followInvestTotalMoney
	 */
	public Double getFollowInvestTotalMoney() {
		return followInvestTotalMoney;
	}

	/**
	 * @param followInvestTotalMoney
	 *            the followInvestTotalMoney to set
	 */
	public void setFollowInvestTotalMoney(Double followInvestTotalMoney) {
		this.followInvestTotalMoney = followInvestTotalMoney;
	}

}
