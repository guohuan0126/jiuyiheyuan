<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.duanrong.business.autoinvest.mapper.AutoInvestHistory">
	<resultMap type="autoInvestHistory" id="AutoInvestRM">
		<id property="id" column="id" />
		<result property="userId" column="user_id" />
		<result property="investMoney" column="investMoney" />
		<result property="time" column="time" />
		<result property="seqNum" column="seq_num" />
		<result property="loanID" column="loan_id" />
		<result property="detail" column="detail" />
		<result property="status" column="status" />
		<result property="requestNo" column="requestNo" />	
	</resultMap>

	

	<insert id="insert" parameterType="autoInvestHistory">
		INSERT INTO auto_invest_history
		(user_id, detail, investMoney, time, seq_num,
		loan_id, status, requestNo)
		VALUES
		(#{userId}, #{detail}, #{investMoney}, #{time}, 
		#{seqNum}, #{loanID}, #{status}, #{requestNo})
	</insert>

  
</mapper>