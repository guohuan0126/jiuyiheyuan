package com.duanrong.business.payMentChannel.model;

/**
 * 银行和渠道汇总信息
 * 
 */
public class PaymentBankChannel {

	private String id;      // 编号
	private String bankId;    // 银行信息表Id
	private String channelId;   // 支付渠道表ID
	private String bankName;
	private String channelName;
	private Double oneMoney;    // 单笔限额
	private Double dayMoney;    // 单日限额
	private Double monthMoney;    // 单月限额
	private String onlineBanking;   // 是否支持网银 1支持 2不支持
	private String quickRecharge;    // 是否支持快捷 1支持 2不支持
	private String tiecard;    // 是否支持绑卡 1支持 2不支持
	private String urgentWithdrawals;   // 是否支持加急提现 1支持 2不支持
	private String sort;   // 排序
	private String isDel;   // 是否删除，1显示 2显示
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getBankId() {
		return bankId;
	}
	public void setBankId(String bankId) {
		this.bankId = bankId;
	}
	public String getChannelId() {
		return channelId;
	}
	public void setChannelId(String channelId) {
		this.channelId = channelId;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public String getChannelName() {
		return channelName;
	}
	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}
	public Double getOneMoney() {
		return oneMoney;
	}
	public void setOneMoney(Double oneMoney) {
		this.oneMoney = oneMoney;
	}
	public Double getDayMoney() {
		return dayMoney;
	}
	public void setDayMoney(Double dayMoney) {
		this.dayMoney = dayMoney;
	}
	public Double getMonthMoney() {
		return monthMoney;
	}
	public void setMonthMoney(Double monthMoney) {
		this.monthMoney = monthMoney;
	}
	public String getOnlineBanking() {
		return onlineBanking;
	}
	public void setOnlineBanking(String onlineBanking) {
		this.onlineBanking = onlineBanking;
	}
	public String getQuickRecharge() {
		return quickRecharge;
	}
	public void setQuickRecharge(String quickRecharge) {
		this.quickRecharge = quickRecharge;
	}
	public String getTiecard() {
		return tiecard;
	}
	public void setTiecard(String tiecard) {
		this.tiecard = tiecard;
	}
	public String getUrgentWithdrawals() {
		return urgentWithdrawals;
	}
	public void setUrgentWithdrawals(String urgentWithdrawals) {
		this.urgentWithdrawals = urgentWithdrawals;
	}
	public String getSort() {
		return sort;
	}
	public void setSort(String sort) {
		this.sort = sort;
	}
	public String getIsDel() {
		return isDel;
	}
	public void setIsDel(String isDel) {
		this.isDel = isDel;
	}
	
	

	
	
	

}
