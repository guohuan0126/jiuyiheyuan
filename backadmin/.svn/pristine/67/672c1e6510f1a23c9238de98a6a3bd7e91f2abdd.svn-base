package com.duanrong.business.message.model;

import java.util.Date;

import base.model.BaseModel;

import com.duanrong.business.user.model.User;

/*
 * @description 短信管理
 * @author zhangzeren
 * @createTime :2016-11-7
 */
public class MessageTemplate extends BaseModel {
	private static final long serialVersionUID = -8860524056087366008L;
	private String id;
	private String description;
	private String name;
	private String status;
	private String template;
	private String templateView;
	private Date lastModifyTime;
	private String textuserId;
	private int nowStatus;
	private String messageWay;
	private String messageNode;
	
	public String getMessageNode() {
		return messageNode;
	}
	public void setMessageNode(String messageNode) {
		this.messageNode = messageNode;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getTemplate() {
		return template;
	}
	public void setTemplate(String template) {
		this.template = template;
		
	}
	 
	
	
	public Date getLastModifyTime() {
		return lastModifyTime;
	}
	public void setLastModifyTime(Date lastModifyTime) {
		this.lastModifyTime = lastModifyTime;
	}
	public String getTemplateView() {
		return templateView;
	}
	public void setTemplateView(String templateView) {
		this.templateView = templateView;
	}
	 
	public String getTextuserId() {
		return textuserId;
	}
	public void setTextuserId(String textuserId) {
		this.textuserId = textuserId;
	}
	
	 
	public int getNowStatus() {
		return nowStatus;
	}
	public void setNowStatus(int nowStatus) {
		this.nowStatus = nowStatus;
	}
	public String getMessageWay() {
		return messageWay;
	}
	public void setMessageWay(String messageWay) {
		this.messageWay = messageWay;
	}
	@Override
	public String toString() {
		return "MessageTemplate [id=" + id + ", description=" + description
				+ ", name=" + name + ", status=" + status + ", template="
				+ template + ", templateView=" + templateView
				+ ", lastModifyTime=" + lastModifyTime + ", textuserId="
				+ textuserId + ", nowStatus=" + nowStatus + ", messageWay="
				+ messageWay + ", messageNode=" + messageNode + "]";
	}
	
	 
	 
	 
	
}
