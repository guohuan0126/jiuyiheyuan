	<sql id="getLoansVehicle">
		loan.id as id, loan.name as name, loan.commit_time as commit_time, loan_type as loan_type, loan.repay_type as repay_type,
		loan.company_name as company_name, loan_vehicle.brand as brand, loan_vehicle.license_plate_number as license_plate_number,
		(select IFNULL(sum(money), 0) from invest where invest.loan_id = loan.id and invest.status != '流标') as calculateMoneyNeedRaised,
		loan.money as money, loan.deadline as deadline, loan.day as day, loan.operation_type as operation_type, loan.rate as rate, 
		loan_vehicle.yacar_and_gps as yacar_and_gps, loan_vehicle.borrower_name, loan_vehicle.remark as remark, 
		loan_vehicle.item_address as item_address, loan_vehicle.money as single_money, loan_vehicle.borrower_id_card as borrower_id_card
	</sql>
	<sql id="getLoansHouse">
		loan.id as id, loan.name as name, loan.commit_time as commit_time, loan_type as loan_type, loan.repay_type as repay_type,
		loan.company_name as company_name, loan_house.position as brand, 
		(select IFNULL(sum(money), 0) from invest where invest.loan_id = loan.id and invest.status != '流标') as calculateMoneyNeedRaised,
		loan.money as money, loan.deadline as deadline, loan.day as day, loan.operation_type as operation_type, loan.rate as rate, 
		loan_house.borrower_name, loan_house.remark as remark, loan_house.item_address as item_address, loan_house.borrower_id_card as borrower_id_card
	</sql>
	<sql id="getLoansEnterprise">
		loan.id as id, loan.name as name, loan.commit_time as commit_time, loan_type as loan_type, loan.repay_type as repay_type,
		loan.company_name as company_name, (select IFNULL(sum(money), 0) from invest where invest.loan_id = loan.id and invest.status != '流标') as calculateMoneyNeedRaised,
		loan.money as money, loan.deadline as deadline, loan.day as day, loan.operation_type as operation_type, loan.rate as rate, 
		loan_enterprise.borrower_name, loan_enterprise.remark as remark, loan_enterprise.item_address as item_address, loan_enterprise.borrower_id_card as borrower_id_card
	</sql>
	<sql id="getLoansSupplychain">
		loan.id as id, loan.name as name, loan.commit_time as commit_time, loan_type as loan_type, loan.repay_type as repay_type,
		loan.company_name as company_name, (select IFNULL(sum(money), 0) from invest where invest.loan_id = loan.id and invest.status != '流标') as calculateMoneyNeedRaised,
		loan.money as money, loan.deadline as deadline, loan.day as day, loan.operation_type as operation_type, loan.rate as rate, 
		loan_supplychain.borrower_name, loan_supplychain.remark as remark, loan_supplychain.item_address as item_address, loan_supplychain.borrower_id_card as borrower_id_card
	</sql>
	
	<select id="getLoansByGroupCondition1" resultMap="loanRM2"
		parameterType="map">
		SELECT 
		<choose>
			<when test="loanType == '车贷' "><include refid="getLoansVehicle"/></when>
			<when test="loanType == '房贷' "><include refid="getLoansHouse"/></when>
			<when test="loanType == '企业贷' "><include refid="getLoansEnterprise"/></when>
			<when test="loanType == '供应宝' "><include refid="getLoansSupplychain"/></when>
			<otherwise><include refid="getLoansVehicle"/></otherwise>
		</choose>		
		FROM loan
		<choose>
			<when test="loanType == '车贷' ">left join loan_vehicle on loan.id = loan_vehicle.loan_id</when>
			<when test="loanType == '房贷' ">left join loan_house on loan.id = loan_house.loan_id</when>
			<when test="loanType == '企业贷' ">left join loan_enterprise on loan.id = loan_enterprise.loan_id</when>
			<when test="loanType == '供应宝' ">left join loan_supplychain on loan.id = loan_supplychain.loan_id</when>
			<otherwise>left join loan_vehicle on loan.id = loan_vehicle.loan_id</otherwise>		
		</choose>	
		<where>		
			<if test="id != null and id != '' ">
				AND loan.id = #{id}
			</if>
			<if test="name != null and name != '' ">
				AND name = #{name}
			</if>
			<choose>
				<when test="loanType == null or loanType == '' or loanType == '车贷' " >
					and loan_type = '车贷' and loan_vehicle.status = 1
				</when>
				<otherwise>
					AND loan_type = #{loanType}
				</otherwise>
			</choose>								
			<if test="status != null and status != '' ">
				AND status = #{status}
			</if>			
			<if test="textItem != null and textItem != '' ">
				AND company_name = #{textItem}
			</if>
			<if test="newbieEnjoy != null and newbieEnjoy != '' ">
				AND newbieEnjoy = #{newbieEnjoy}
			</if>		
			<if test="deadline != null and deadline != '' ">
				AND operation_type = '月' AND deadline = #{deadline}
			</if>
			<if test="minMoney != null and minMoney != '' ">
				AND money &gt;= #{minMoney}
			</if>			
			<if test="maxMoney != null and maxMoney != '' ">
				AND money &lt;= #{maxMoney}
			</if>
			<choose>
				<when test="start != null and start != '' ">
					AND commit_time &gt;= #{start}
				</when>
				<when test="end != null and end != '' ">
					AND commit_time &lt;= #{end}
				</when>			
				<otherwise>
					AND date(commit_time) = curdate()
				</otherwise>
			</choose>
		</where>
	</select>
	

