	<!-- 根据常用invest表中字段进行组合条件查询 -->
	<select id="getInvestsByGroupCondition" resultMap="com.duanrong.business.invest.model.Invest.investRM"
		parameterType="com.duanrong.business.invest.model.Invest">
		SELECT * FROM
		invest
		<where>
			<if test="id != null and id != ''">
				AND id =#{id}
			</if>
			<if test="loanId != null and loanId != ''">
				AND loan_id = #{loanId}
			</if>

			<if test="status eq '!流标'">
				AND status IN('投标成功','还款中','完成','等待确认')
			</if>
		</where>
		order by time desc
	</select>


	<select id="generateId" resultMap="loanRM" parameterType="string">
		select
		*
		from loan where id = (select max(l.id) from loan as l where l.id like
		CONCAT(#{str},'%')) for update
	</select>


	<resultMap type="fixedborrowers" id="fbRM">
		<id property="userId" column="user_id" />
		<id property="status" column="status" />
		<result property="email" column="email" />
		<association property="fixedUser"
			resultMap="com.duanrong.business.user.mapper.UserMapper.userRM"></association>
	</resultMap>

	<select id="findAllFixedBorrowers" resultMap="fbRM">
		SELECT
		fb.*,
		fixedUser.*
		FROM
		fixed_borrowers AS fb
		INNER JOIN `user` AS fixedUser ON
		fb.user_id = fixedUser.id where fb.status = 'open'
	</select>

	<insert id="insertFixedBorrower" parameterType="string">
		INSERT INTO
		fixed_borrowers (`user_id`, `status`, email) VALUES (#{userId}, 'open', #{email});
	</insert>

	<update id="alterFixedBorrowersStatus" parameterType="map">
		UPDATE fixed_borrowers
		<set>
			<if test="status!=null">status=#{status},</if>
			<if test="email!=null and email != '' ">email=#{email},</if>
		</set>
		WHERE user_id=#{borrower};
	</update>
	<select id="getTotalMoney" parameterType="map"
		resultType="double">
		select ROUND(SUM(money), 2) from loan
		<where>
			<if test="type != null and type != '' and type!='all'">
				AND loan_type =#{type}
			</if>
			<if test="status eq '流标'">
				AND status!='流标'
			</if>
			<if test="status eq '还款中'">
				AND status='还款中'
			</if>
			<if test="1 eq 1">
				and company_name!='是'
			</if>
		</where>
	</select>
	<select id="find" resultMap="loanRM" parameterType="loan">
		select t.*,round((t.Maxcorpus+t.Maxinterest),2) as Maxreapymoney from (	SELECT loan.*, user.realname as borrowMoneyUserName,loan_vehicle.brand as brand,loan_vehicle.license_plate_number as license_plate_number,
(select operation_time from repay where loan_id=loan.id ORDER BY repay_day desc  limit 1) as Maxrepayday,
 (select  corpus from repay where loan_id=loan.id  ORDER BY repay_day desc  limit 1) as Maxcorpus,
(select  interest from repay where loan_id=loan.id  ORDER BY repay_day desc  limit 1) as Maxinterest
 FROM loan left join user on loan.user_id=user.id left join loan_vehicle on loan.id=loan_vehicle.loan_id) t
		<where>
			<if test="start != null and start != '' ">
				and t.give_money_operation_time &gt;=#{start}
			</if>
			<if test="end != null and end != '' ">
				and t.give_money_operation_time &lt;=#{end}
			</if>
			<if test="statusStr != null and statusStr =='statusStr' ">
				and t.status in('还款中','完成')
			</if>
		</where>
		ORDER BY t.give_money_operation_time DESC
	</select>
	
	<select id="getWaitPushLoanList" parameterType="java.lang.Object" resultMap="loanRM">
		select l.* from loan l 
		 where not EXISTS (select 1 from push_loan p where p.loan_id=l.id and p.type='p2peye')
		   and l.`status` in('筹款中','贷前公告')
			and l.company_name='否'
	</select>
	
	<select id="getPushRecords" parameterType="java.lang.Object" resultMap="loanRM">
		select l.*,p.push_time from loan l 
		 inner join push_loan p 
		    on l.id=p.loan_id
		<include refid="query_push_records"/>
		order by p.push_time desc
	</select>
	
	<select id="getP2pEyeLoanByLoanId" parameterType="java.lang.Object" resultMap="loanRM">
		select l.id,l.`name`,l.money,l.rate,l.repay_type,l.operation_type,l.deadline,l.`day`,l.loan_type,l.`status`,l.commit_time,l.type from loan l
			where l.id = #{id }
	</select>
	<select id="getNetLoanCount" parameterType="java.lang.Object" resultMap="netLoan">
		select count(p.id) pushLoanNum,sum(l.money) pushLoanSumMoney
		  from push_loan p INNER JOIN loan l on p.loan_id=l.id
<!-- 		  <include refid="query_push_records"/> -->
	</select>
	<select id="getNetLoanSum" parameterType="java.lang.Object" resultMap="netLoan">
		select count(DISTINCT iv.user_id) investNum, (select count(1) from platform_user_relation pt INNER JOIN user u on pt.userID=u.id
		where pt.whether_new='0') regUserCount from invest iv inner join platform_user_relation pt on iv.user_id=pt.userID 
	</select>
	
	<sql id="query_push_records">
		<where>
			p.type='p2peye'
			<if test="id != null and id != '' ">
				and l.id=#{id}
			</if>
			<if test="name != null and name != '' ">
				and l.name like '%${name}%'
			</if>
			<if test="status != null and status != '' ">
				and l.status=#{status}
			</if>
			<if test="start != null and start != '' ">
				and l.give_money_operation_time &gt;=#{start}
			</if>
			<if test="end != null and end != '' ">
				and l.give_money_operation_time &lt;=#{end}
			</if>
			<if test="pushTimeStart != null and pushTimeStart != '' ">
				and p.push_time &gt;=#{pushTimeStart}
			</if>
			<if test="pushTimeEnd != null and pushTimeEnd != '' ">
				and p.push_time &lt;=#{pushTimeEnd}
			</if>
			<if test="loanType != null and loanType != '' ">
				and l.loan_type =#{loanType }
			</if>
		</where>
	</sql>
	<select id="getLoanName"  resultType="java.lang.String">
		SELECT name from loan where loan_type='车贷'  order by commit_time desc limit 0,1
	</select>
</mapper>