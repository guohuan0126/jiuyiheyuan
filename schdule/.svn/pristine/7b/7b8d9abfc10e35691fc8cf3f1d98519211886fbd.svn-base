package util;

import java.util.Collection;

import org.springframework.util.CollectionUtils;

/**
 * @Description: Collection集合工具类
 * @Author: 林志明
 * @CreateDate: Nov 25, 2014
 */
public class MyCollectionUtils extends CollectionUtils {

	/**
	 * 判断集合是否不为空（不为空返回true）
	 * 
	 * @param collection
	 *            实现Collection接口的集合
	 * @return
	 */
	public static boolean isNotBlank(Collection<? extends Object> collection) {
		return !isEmpty(collection);
	}
}