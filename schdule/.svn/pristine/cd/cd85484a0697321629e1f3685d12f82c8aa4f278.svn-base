package com.duanrong.business.award;

import com.duanrong.business.award.model.ItemType;

public class ItemTypeUtil {

	
	public static ItemType getItemType(String type){
		
		if(type.equals("award")){
			return ItemType.AWARD;
		}else if(type.equals("undeal")){
			return ItemType.UNDEAL;
		}else if(type.equals("approve")){
			return ItemType.APPROVE;
		}else if(type.equals("unapproved")){
			return ItemType.UNAPPROVED;
		}
		return null;
	}
}
