} 
/*数据展示*/
.left{float:left;}
.right{float:right;}
.data{width:100%;padding-top:30px;}
.data-top{width:97%;height:38px;line-height:38px;background:#4fb6ff url(../images/shuju-title.jpg) no-repeat 30px center;font-size:18px;color:#fff;padding-left:2.7%;}
.data-all{width:97.8%;border:1px solid #cdcdcd;border-top:0;padding:16px 0 16px 30px;background:#fff;}
ul.data-box{width:100%;overflow:hidden;}
ul.data-box li{float:left;border:1px solid #dcdcdc;background:#fff;border-radius:3px;}
ul.data-box li.box-samll{width:232px;margin-right:28px;margin-bottom:20px;}
ul.data-box li.box-big{width:298px;margin-right:20px;margin-bottom:20px;}
ul.data-box li p.small-top{height:32px;line-height:32px;font-size:16px;color:#232327;background:#efefef;padding-left:18px;}
ul.data-box li p.small-bottom{height:66px;line-height:66px;color:#3498df;font-size:30px;padding-left:18px;}
.data-ul-bottom{width:98%;border:1px solid #dcdcdc;border-radius:3px;height:56px;line-height:56px;margin-bottom:20px;}
.data-ul-bottom span{float:left;font-size:16px;background:url(../images/data-line.jpg) no-repeat right center;}
.data-ul-bottom span.have-bg{width:92px;background:#efefef;text-align:center;color:#2b2b2b;}
.data-ul-bottom span.login-peo,.data-ul-bottom span.add-peo,.data-ul-bottom span.invest-peo{width:176px;padding:0 18px;height:56px;line-height:56px;}
.data-ul-bottom span b.left{background:url(../images/ioc-3.png) no-repeat 0 20px;padding-left:20px;}
.data-ul-bottom span.add-peo b.left{background:url(../images/ioc-3.png) no-repeat 0 -21px;padding-left:20px;}
.data-ul-bottom span.invest-peo b.left{background:url(../images/ioc-3.png) no-repeat 0 -59px;padding-left:20px;}
.data-ul-bottom span b.right strong{color:#1a1a1b;font-size:22px;}
.data-ul-bottom span.no-bg{background:none;}
.data-box-table{width:98%;border:1px solid #dcdcdc;border-radius:3px;border-bottom:0;}
.data-table-title{padding-left:18px;font-size:16px;color:#232327;background:#efefef;height:32px;line-height:32px;margin-bottom:17px;}
.data-box-table table tr td{ text-align:center; vertical-align:middle;border-right:1px solid #cdcdcd;border-bottom:1px solid #dcdcdc;font-size:18px;padding:15px 0;}
.data-box-table table tr.table-title td span{font-size:17px;color:#333;padding:5px 0;background:url(../images/preduct-1.png) no-repeat;width:90px;margin:0 auto;text-align:right;display:block;}
.data-box-table table tr.table-title td.txt-none{color:#fff;background:none;}
.data-box-table table tr.table-title td.car span{background-position:10px 8px;}
.data-box-table table tr.table-title td.house span{background-position:10px -17px;}
.data-box-table table tr.table-title td.company span{background-position:10px -42px;}
.data-box-table table tr.table-title td.flexible span{background-position:10px -93px;}
.data-box-table table tr.table-title td.supply span{background-position:10px -68px;}
.data-box-table table tr.table-title td.agriculture span{background-position:10px -121px;}
.data-box-table table tr td:last-child{border-right:0;}
.data-box-table table tr td.blue-color{color:#3498df;}
.data-box-table table tr td.lj-bg{background:url(../images/shuju-1.jpg) no-repeat 35px center;}
/*数据查询*/
.block-box{width:100%;height:50px;}
.block-box1{padding-left:36px;}	
.block-box1 label{margin-right:36px;}
.box .small-list{margin:0 36px 20px 0px;}
.box .small-list-right{margin:0 0px 20px 36px;}
.box .input-list input.small-but{width:100px;letter-spacing:1px;}

