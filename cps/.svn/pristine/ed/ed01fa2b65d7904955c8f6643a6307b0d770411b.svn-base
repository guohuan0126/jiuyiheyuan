@charset "utf-8";
/* CSS Document */
html,body,div,p,form,label,ul,li,dl,dt,dd,ol,img,button,h1,h2,h3,h4,h5,h6,b,em,strong,small,input,select{margin:0;padding:0;border:0;list-style:none;font-style:normal;font-weight:normal;}
dl,dt,dd{display:block;margin:0;}
a{text-decoration:none;}
body{font-family:Microsoft YaHei,Verdana,Arial,SimSun;color:#666;font-size:14px;background:#f6f6f6;}
.block, #block{display:block;}
.none, #none{display:none;}
/*****头部和尾部******/
.top{height:5px;line-height:5px;background:#f6f6f6;width:100%;position:fixed;z-index:255;top:79px;text-align:center;}
.bottom{height:5px;line-height:5px;background:#f6f6f6;width:100%;position:fixed;z-index:255;bottom:50px;text-align:center;}
#header{height:78px;line-height:80px;background:#3498df;width:100%;position:fixed;z-index:20;top:0;text-align:center;box-shadow:0 0 2px #000;}
.logo{line-height:80px;font-size:30px;color:#fff; letter-spacing:3px;margin-left:50px;float:left;background:url(../images/logo.png) no-repeat 0;padding-left:155px;}
.navigation{float:right;margin:0px 50px 0 0;color:#fff;font-size:16px;height:80px;line-height:80px;}
.navigation ul li{height:80px;line-height:80px;text-align:center;float:left;margin-left:15px;}
.navigation ul li span{margin-left:15px;}
.navigation ul li a{color:#fff;}
/*.navigation ul li a:hover{color:#32323a;}*/
#footer{height:50px;line-height:50px;background:#5e5e5e;width:100%;position:absolute;z-index:200;bottom:0;text-align:center;}
#footer p{text-align:center;line-height:50px;color:#fbfbfb;}
/*****主体部分******/
#content{ height:100%;width:100%;margin-top:78px;}
/**左侧菜单***/
.left_menu{float:left;width:200px;background:#fff;height:100%;overflow:auto;border-right:1px solid #e3e3e3;}
.left_menu ul li {width:200px; display:inline; }
.left_menu ul li .list-item a{width:140px;text-decoration:none;font-size:16px;color:#4b4b56;height:36px;line-height:36px;display:block;padding-left:60px;background:url(../images/ioc-1.png) no-repeat 34px 12px;}
.left_menu ul li .list-item a.on,.left_menu ul li .list-item a:hover{background:url(../images/ioc-1.png) no-repeat 34px -14px;color:#3498df;}
.left_menu ul li a.noline { border-bottom:none; }
/*.left_menu ul li a:hover{ color:#fff; }*/
.left_menu ul li a.selected:hover { color:#fff; }
.left_menu ul li h4 { cursor:pointer; /*background:url(../images/bg1.png) no-repeat 270px 18px; padding-left:60px;*/ text-decoration:none; font-size:18px; color:#32323a; display:block;  line-height:48px; font-weight:normal; }
.left_menu ul li.noline { border-bottom:none; }
.left_menu ul li.selected h4 {background:url(../images/selected.jpg) no-repeat;color:#fff;}
.left_menu li .list-item { padding:8px 0; position:relative; zoom:1 ;background:#f5f6fa;overflow:hidden;}
.left_menu h4 span{display: block;float: left;width: 20px;height: 26px;margin:12px 10px 0 20px;background-repeat: no-repeat;}
.M1 span{background:url(../images/ioc.png) 0 2px;}
.M2 span{background:url(../images/ioc.png) 0px -25px;}
.M3 span{background:url(../images/ioc.png) 0px -49px;}
.M4 span{background:url(../images/ioc.png) 0px -77px;}
.M5 span{background:url(../images/ioc.png) 0px -102px;}
.M6 span{background:url(../images/ioc.png) 0px -128px;}
.left_menu ul li.selected .M1 span{background:url(../images/ioc.png) 0 -155px;}
.left_menu ul li.selected .M2 span{background:url(../images/ioc.png) 0px -182px;}
.left_menu ul li.selected .M3 span{background:url(../images/ioc.png) 0px -206px;}
.left_menu ul li.selected .M4 span{background:url(../images/ioc.png) 0px -233px;}
.left_menu ul li.selected .M5 span{background:url(../images/ioc.png) 0px -259px;}
.left_menu ul li.selected .M6 span{background:url(../images/ioc.png) 0px -285px;}
/*.M7 span{background:url(../images/ioc.png) -1px -70px;}
.M8 span{background:url(../images/ioc.png) -36px -70px;}
.M9 span{background:url(../images/ioc.png) -72px -70px;}
.M10 span{background:url(../images/ioc.png) -3px -105px;}
*//***右侧内容****/
.m-right{margin-left:200px;}
.right-nav{height:30px;line-height:30px;background:#f6f6f6;padding:30px 0;}
.right-nav li{float:left;margin-left:30px;padding-left:15px;line-height:30px;color:#464646;font-size: 24px;background:url(../images/on-bg.jpg) no-repeat 0;}
.right-nav li img{position:relative;top:17px;left:15px;}
.right-nav li a{color:#464646;}
.right-nav li a:hover{color:#00a5a5; text-decoration:underline;}
.main{width:100%;height:100%;background:#f6f6f6;}
/*正文*/
.bound{padding:0 30px 600px;height:100%;}
.box{width:100%;background:#fff;border-radius:3px;padding:20px 0 0;min-height:98px;}
.box .input-list{/* display:inline-block; */margin:0 36px 20px;float: left;}
.box .input-list input{display:inline-block;height:28px;line-height:25px;border-radius:3px;font-family:"微软雅黑";}
.box .input-list input[type="text"]{border:1px solid #dcdcdc;margin-left:5px;padding-left:3px;}   
.box .input-list input[type="button"]{cursor:pointer;} 
.box .input-list input.clear{background:#f5f6fa;border:1px solid #dcdcdc;color:#32323a;letter-spacing:10px;width:150px;} 
.box .input-list input.clear:hover{background:#f6f8ff;}
.box .input-list input.clear:active{background:#eceef3;}
.box .input-list input.but-search{background:#3498df;color:#fff;height:30px;line-height:30px;letter-spacing:10px;width:150px;}  
.box .input-list input.but-search:hover,input.read:hover,input.add:hover{background:#39a1eb;}
.box .input-list input.but-search:active,input.read:active,input.add:active{background:#208cd8;}   
.box-title{padding:0 0 20px 20px;height: 30px;line-height: 30px;}
.box-title span{float:left;}
span.user{float:left;height:28px;line-height:28px;}
input.read,input.add{background:#3498df;color:#fff;height:30px;line-height:30px;letter-spacing:0;width:90px;margin-left:20px;border-radius:3px; cursor:pointer;float:left;}
.box .input-list input.time-begin{background:url(../images/begin-time.jpg) no-repeat right 0; cursor:pointer;}
.box .input-list input.time-over{background:url(../images/time-over.jpg) no-repeat right 0;cursor:pointer;}
select.short{width: 154px;text-align: center;height: 30px;color: #777;font-size: 14px;font-family: "微软雅黑";border-radius:3px;margin-left:20px;border:1px solid #ccc;}
select.long{width:335px;height:28px;line-height:25px;border-radius:3px;font-family:"微软雅黑";border:1px solid #dcdcdc;border-radius:3px;margin-left:5px;padding-left:3px;}
.box-table{padding:0 20px;}
.box-table table{border:1px solid #ccc;}
.box-table table tr{height:42px;}
.box-table table tr.table-tit{background:#e1e3ed;}
.box-table table tr td{text-align:center;font-size:16px;border-bottom:1px solid #dcdcdc;}
.box-table table tr td input{width:15px;height:15px;vertical-align: middle;}
.table-pages{padding-left:20px;background:#f6f8ff;height:42px;line-height:42px;border:1px solid #dcdcdc;border-top:0;margin-bottom:20px;}
.pages-sec{border:0;background:none;padding-left:6px;color: #777;font-size: 14px;font-family: "微软雅黑";}
.table-pages span,.table-pages select{float:left;}
.table-pages span.pages-all{border:1px solid #dcdcdc;height:28px;line-height:28px;margin:0;border-right:0;overflow:hidden;}
.table-pages select{margin-top:11px;}
.table-pages span.pages-all{margin:6px 15px 0;}
.table-pages span a{float:left;width:28px;height:28px;text-align:center;line-height:28px;border-right:1px solid #dcdcdc;cursor:pointer;}
.table-pages span a.cur{background:#3498df;color:#fff;}
.table-pages span.pages-right .input-txt{width:60px;heihgt:28px;line-height:28px;border:1px solid #dcdcdc;margin:6px 8px 0 0;}
.table-pages span.pages-right .input-but{width:30px;heihgt:28px;line-height:28px;border:1px solid #dcdcdc;margin:6px 8px 0 0;background:#fff;color:#3498df; cursor:pointer;}
/*登录*/
.login-bound{width:100%;height:100%;background:url(../images/login.jpg) no-repeat center;position:absolute;top:0;left:0;}
.login-box-big{width: 434px;height: 434px;background:#7eafe2;position:absolute;top:50%;left:50%;margin-left: -217px;margin-top:-217px;border-radius:10px;}
.login-box-small{width: 415px;height: 408px;background:#a6cbeb;border-radius:10px;margin:9.5px auto;padding-top: 7px;}
.login-box{width: 401px;height: 400px;background:#fff;border-radius:10px;margin:0 auto;}   
.login-box h1{padding-top:55px;}                          
.login-box h1 img{display:block;margin:0 auto;}
.login-box h3{font-size:22px;color:#7eafe2;text-align:center;height:48px;line-height:48px; letter-spacing:2px;}
.box-main{padding-top:20px;width:364px;margin:0 auto;}
.box-main input{margin-bottom: 15px;width:311px;height:46px;line-height:46px;border:1px solid #d8d8d8;border-radius:3px;background:url(../images/ioc-2.png) no-repeat;padding-left:50px;font-size:18px;color:#b2b3b3;font-family:"微软雅黑";}
.box-main input.name{background-position:16px 12px;}
.box-main input.password{background-position:16px -23px;}
.box-main input.login-but{background:url(../images/login-btn.png) no-repeat;color:#fff;width:364px;height:50px;line-height:48px;text-align:center;border:none;border-radius:0;padding:0; letter-spacing:6px;}


/*------checked 样式-----*/
.zhanghao{width: 362px;overflow: hidden;margin-bottom: 20px;padding-left: 2px;font-size:15px;color:#b2b3b3;}
.clearfix{float:left;height:18px;line-height:18px;}
.clear-right{float:right;height:18px;line-height:18px;}
.bdland_piaochecked {
width: 17px;
height: 18px;
display:inline-block;
cursor: pointer;
margin:0px 3px -3px 0px;
text-align: center;
background:url(../images/checked.png) no-repeat;
background-position:0 1px;

}

.bdland_on_check {
width: 17px;
height: 18px;
display:inline-block;
cursor: pointer;
margin:0px 3px -3px 0px;
text-align: center;
background:url(../images/checked.png) no-repeat;
background-position: 0 -19px;
}

/**/
.box-title .btn-select { 
position: relative; 
display: inline-block; 
width: 150px; 
height: 28px; 
background-color: #fff; 
font: 14px/20px "Microsoft YaHei"; 
color: #2d2d2d; 
margin-left: 15px;
} 
.box-title .btn-select .cur-select { 
position: absolute; 
display: block; 
width: 150px; 
height: 27px; 
border:1px solid #cdcdcd;
line-height: 25px; 
background: #fff url(../images/ico-arrow.jpg) no-repeat 125px center; 
text-indent: 10px; 
/* margin-left: 15px; */
} 

.box-title .btn-select select { 
position: absolute; 
top: 0; 
left: 0; 
width: 150px; 
height: 25px; 
opacity: 0; 
filter: alpha(opacity: 0;); 
font: 14px/20px "Microsoft YaHei"; 
color: #2d2d2d; 
} 
.btn-select select option { 
text-indent: 10px; 
} 

