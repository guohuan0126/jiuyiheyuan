package com.duanrong.cps.business.netloaneye.model;

import java.io.Serializable;

@SuppressWarnings("serial")
public class UserCookie implements Serializable {

	private String userId;
	private String email;
	private boolean invest;
	private boolean member;
	private String userType;
	private String mobileNumber;
	
	
	//手机号
	public String getMobileNumber() {
		return mobileNumber;
	}

	public void setMobileNumber(String mobileNumber) {
		this.mobileNumber = mobileNumber;
	}
	
	

	/**
	 * enterprise，企业用户，user或者为空是普通用户
	 * @return
	 */
	public String getUserType() {
		return userType;
	}

	public void setUserType(String userType) {
		this.userType = userType;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public boolean isInvest() {
		return invest;
	}

	public boolean isMember() {
		return member;
	}

	public void setInvest(boolean invest) {
		this.invest = invest;
	}

	public void setMember(boolean member) {
		this.member = member;
	}
}