<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.duanrong.drpay.business.platformtransfer.mapper.PlatformTransferMapper">
	<resultMap type="PlatformTransfer" id="platformTransferRM">
		<id property="id" column="id" />
		<result property="orderId" column="order_id" />
		<result property="userId" column="username" />
		<result property="time" column="time" />
		<result property="successTime" column="success_time" />
		<result property="status" column="status" />
		<result property="actualMoney" column="actual_money" />
		<result property="remarks" column="remarks" />
		<result property="type" column="type" />
	</resultMap>

	<insert id="insert" parameterType="platformTransfer">
		INSERT INTO platform_transfer
		(id,order_id,username,time,
		success_time,status,actual_money, remarks, type)
		VALUES
		(#{id},#{orderId},#{userId},#{time},#{successTime},#{status},#{actualMoney},#{remarks}, #{type})
	</insert>

	<insert id="insertBatch" parameterType="list">
		INSERT INTO platform_transfer
		(id,order_id,username,time,
		success_time,status,actual_money, remarks, type)
		VALUES
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.id},#{item.orderId},#{item.userId},#{item.time},#{item.successTime},#{item.status},#{item.actualMoney},#{item.remarks}, #{item.type})
		</foreach>
	</insert>
</mapper>