package base.exception;

import base.error.ErrorCode;

/**
 * 交易异常
 * @author xiao
 * @datetime 2016年11月8日 上午10:04:03
 */
public class TradeException extends ErrorCodeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 9037324548272087684L;

	public TradeException() {
	}

	public TradeException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

	public TradeException(String message, Throwable cause) {
		super(message, cause);
	}

	public TradeException(String message) {
		super(message);
	}

	public TradeException(Throwable cause) {
		super(cause);
	}

	public TradeException(ErrorCode code) {
		super(code);
	}

	public TradeException(ErrorCode code, String message) {
		super(code, message);
	}
}
