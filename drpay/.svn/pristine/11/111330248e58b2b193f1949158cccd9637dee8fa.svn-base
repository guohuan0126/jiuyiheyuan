<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.duanrong.drpay.business.trusteeship.mapper.TrusteeshipOperationMapper">
	<resultMap type="trusteeshipOperation" id="trusteeshipOperationRM">
		<id property="id" column="trusteeshipOperation_id" />
		<result property="type" column="type" />
		<result property="markId" column="mark_id" />
		<result property="operator" column="operator" />
		<result property="status" column="status" />
		<result property="requestUrl" column="request_url" />
		<result property="requestTime" column="request_time" />
		<result property="requestData" column="request_data" />
		<result property="responseTime" column="response_time" />
		<result property="responseData" column="response_data" />
		<result property="trusteeship" column="trusteeship" />
		<result property="userId" column="user_id" />
	</resultMap>

	<insert id="insert" parameterType="trusteeshipOperation">
		INSERT INTO trusteeship_operation
		(id,type,mark_id,operator,status,request_url,request_time,request_data,trusteeship,user_id)
		VALUES
		(REPLACE(UUID(),'-',''),#{type},#{markId},#{operator},#{status},#{requestUrl},#{requestTime},#{requestData},#{trusteeship},#{userId})
	</insert>
</mapper>