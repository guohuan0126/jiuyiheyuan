package base.exception;

import base.error.ErrorCode;

/**
 * 项目相关异常
 * @author xiao
 * @datetime 2016年12月13日 上午9:39:15
 */
public class LoanException extends ErrorCodeException{

	/**
	 * 
	 */
	private static final long serialVersionUID = 5617619262598394411L;

	public LoanException() {
		super();
	}

	public LoanException(ErrorCode code, String message) {
		super(code, message);
	}

	public LoanException(ErrorCode code) {
		super(code);
	}

	public LoanException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

	public LoanException(String message, Throwable cause) {
		super(message, cause);
	}

	public LoanException(String message) {
		super(message);
	}

	public LoanException(Throwable cause) {
		super(cause);
	}

	
}
