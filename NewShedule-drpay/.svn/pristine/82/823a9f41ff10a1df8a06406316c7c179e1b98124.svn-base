<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.duanrong.business.payment.mapper.PaymentYeepayTransferUserMapper">
	<resultMap type="PaymentYeepayTransferUser" id="PaymentYeepayTransferUserRM">
		<id property="id" column="id" />
		<result property="requestNo" column="request_no" />
		<result property="paymentNo" column="payment_no" />
		<result property="money" column="money" />
		<result property="userId" column="user_id" />
		<result property="status" column="status" />
		<result property="msg" column="msg" />
		<result property="paymentId" column="payment_id" />
		<result property="createTime" column="create_time" />
	</resultMap>

	<insert id="insert" parameterType="PaymentYeepayTransferUser">
		INSERT INTO payment_yeepay_transfer_user
		(id,request_no,money,user_id,status,msg,payment_id,create_time)
		VALUES
		(#{id},#{requestNo},#{money},#{userId},#{status},#{msg},#{paymentId},#{createTime})
	</insert>
	
	<!-- 获取垫付资金和阀值 -->
	<select id="getByRequestNo" parameterType="String" resultMap="PaymentYeepayTransferUserRM">
		SELECT * from payment_yeepay_transfer_user where request_no = #{requestNo} 
	</select>
</mapper>