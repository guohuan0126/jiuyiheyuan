package com.duanrong.dataAnalysis.business.CapitalData.model;

import java.math.BigDecimal;

//资金数据表
public class CapitalData {
	//今日充值金额
	private double tpayMoney;
	//今日提体现金额
	private double tdrawMoney;
	//昨日充值金额
	private double lpayMoeny;
	//昨日提现金额
	private double ldrawMoney;
	//平台用户可用总金额(大数字类型)
	private BigDecimal userAllMoney;
	public double getTpayMoney() {
		return tpayMoney;
	}
	public void setTpayMoney(double tpayMoney) {
		this.tpayMoney = tpayMoney;
	}
	public double getTdrawMoney() {
		return tdrawMoney;
	}
	public void setTdrawMoney(double tdrawMoney) {
		this.tdrawMoney = tdrawMoney;
	}
	public double getLpayMoeny() {
		return lpayMoeny;
	}
	public void setLpayMoeny(double lpayMoeny) {
		this.lpayMoeny = lpayMoeny;
	}
	public double getLdrawMoney() {
		return ldrawMoney;
	}
	public void setLdrawMoney(double ldrawMoney) {
		this.ldrawMoney = ldrawMoney;
	}
	public BigDecimal getUserAllMoney() {
		return userAllMoney;
	}
	public void setUserAllMoney(BigDecimal userAllMoney) {
		this.userAllMoney = userAllMoney;
	}
	@Override
	public String toString() {
		return "CapitalData [tpayMoney=" + tpayMoney + ", tdrawMoney="
				+ tdrawMoney + ", lpayMoeny=" + lpayMoeny + ", ldrawMoney="
				+ ldrawMoney + ", userAllMoney=" + userAllMoney + "]";
	}
	
	

	
	
}
